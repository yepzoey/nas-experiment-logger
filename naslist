# mount all plt3-owned NASes

sudo mount -t cifs //merfish1.ucsd.edu/merfish1 /mnt/merfish1 -o username=bli,uid=1000

sudo mount -o nosuid,nodev,bg merfish2.ucsd.edu:/volume1/merfish2rna /mnt/merfish2rna
